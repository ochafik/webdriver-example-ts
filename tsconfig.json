{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "build",
    "pretty": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "jsx": "react"
  }
}
